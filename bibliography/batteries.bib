file = {:Users/mbercx/Library/Application Support/Mendeley Desktop/Downloaded/Li et al. - 2018 - Direct Visualization of the Reversible O2−O−Redox Process in Li-Rich Cathode Materials.pdf:pdf},
isbn = {1533-4406 (Electronic)$\backslash$r0028-4793 (Linking)},
issn = {15214095},
journal = {Adv. Mater.},
keywords = {Li-ion batteries,Li-rich materials,in situ Raman spectroscopy,in situ X-ray diffraction,oxygen redox},
number = {14},
pmid = {18946066},
title = {{Direct Visualization of the Reversible O2−/O−Redox Process in Li-Rich Cathode Materials}},
volume = {30},
year = {2018}
}
